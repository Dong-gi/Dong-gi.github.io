mixin w3a(color)
    if color
        a.w3-btn.w3-round.w3-round-xxlarge.w3-small(class=color)&attributes(attributes)
            if block
                block
    else
        a.w3-btn.w3-round.w3-round-xxlarge.w3-small.w3-green&attributes(attributes)
            if block
                block

mixin w3img(src, alt)
    .w3-center
        img.w3-round(src=src, alt=alt)&attributes(attributes)

mixin w3button(color)
    if color
        button.w3-btn.w3-round.w3-round-xxlarge.w3-small(class=color)&attributes(attributes)
            if block
                block
    else
        button.w3-btn.w3-round.w3-round-xxlarge.w3-small.w3-teal&attributes(attributes)
            if block
                block

mixin codeBtn(path, lan, displayRange)
    +w3button()&attributes({ class: 'btn-code', title: path, displayRange: displayRange, lan: lan || 'text' })&attributes(attributes)
        if block
            block
        else
            |View Code »

mixin asA(href, text)
    - if (!text && /#/.test(href)) { text = href.replace(/.*#/, '') }
    - if (!text && /\//.test(href)) { text = href.replace(/.*\//, '') }
    - if (!text) { text = 'Reference' }
    - text = text.replace(/\.(html?|jsp|php|asp|do|js|css|txt|java)$/, '')
    +w3a()&attributes({ href: href, target: '_blank' })&attributes(attributes)= text

mixin ths(...cells)
    tr&attributes(attributes)
        each content, idx in cells
            th= content

mixin tds(...cells)
    tr&attributes(attributes)
        each content, idx in cells
            td= content

mixin hoverTemplate()
    div.w3-container.w3-leftbar.w3-border-green.w3-white.hover-template&attributes(attributes)
        if block
            block

mixin asCode(lan)
    - lan = lan || 'text'
    .as-code.code-div.w3-leftbar.w3-border-green(lan=lan)
        if block
            block

