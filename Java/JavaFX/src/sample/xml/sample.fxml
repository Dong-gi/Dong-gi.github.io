<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <VBox alignment="CENTER" prefWidth="200.0" spacing="10.0">
          <children>
              <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#alertExample" text="Alert" textAlignment="CENTER" />
              <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#textInputDialogExample" text="TextInputDialog" textAlignment="CENTER" />
              <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#choiceDialogExample" text="ChoiceDialog" textAlignment="CENTER" />
              <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#customDialogExample" text="Custom Dialog" textAlignment="CENTER" />
              <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#imageButtonExample" text="Image Button" textAlignment="CENTER" />
              <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#checkBoxExample" text="CheckBox" textAlignment="CENTER" />
              <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#radioButtonExample" text="RadioButton" textAlignment="CENTER" />
              <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#comboBoxExample" text="ComboBox" textAlignment="CENTER" />
              <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#listViewExample" text="ListView" textAlignment="CENTER" />
              <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#sliderExample" text="Slider&#10;Progress *&#10;ScrollBar&#10;Spinner" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS" />
              <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#treeTableViewExample" text="Tree + Table View" textAlignment="CENTER" />
          </children>
          <padding>
              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </padding>
      </VBox>
       <VBox alignment="CENTER" prefWidth="200.0" spacing="10.0">
           <children>
               <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#intersectExample" text="Intersects" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS" />
               <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#timelineExample" text="Timeline" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS" />
               <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#parallelTransigionExample" text="ParallelTransition" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS" />
           </children>
           <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
           </padding>
       </VBox>
   </children>
</HBox>
