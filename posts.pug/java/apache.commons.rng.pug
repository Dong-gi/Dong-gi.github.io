include ../../source/skeleton.pug
+post('Apache Commons RNG', 'Apache Commons RNG', 'Apache Commons RNG 정리')
    h1 org.apache.commons.rng
    div
        h2 interface UniformRandomProvider Since 1.0
        +asCode('java').
            void nextBytes(byte[] bytes) // + 오버로딩
            int nextInt()                // + 오버로딩
            // + nextLong, nextBoolean, nextFloat, nextDouble

        h2 interface RestorableUniformRandomProvider extends UniformRandomProvider Since 1.0
        p 난수 생성기 상태를 저장/복원할 수 있다
        +asCode('java').
            RandomProviderState saveState()
            void restoreState(RandomProviderState state)

    h1 org.apache.commons.rng.core.source32
    ol
        li class ISAACRandom extends IntProvider Since 1.0
        p 암호적으로 안전한 ISAAC 알고리즘 의사 난수 생성기
        li class JDKRandom extends IntProvider Since 1.0
        p Random.nextInt()를 이용하는 의사 난수 생성기
        li class KISSRandom extends IntProvider Since 1.0
        p KISS 알고리즘 의사 난수 생성기
        li class MersenneTwister extends IntProvider Since 1.0
        p 메르센 소수를 이용한 의사 난수 생성기. 사용 시 저작권 명시 제약이 있다
        li class MultiplyWithCarry256 extends IntProvider Since 1.0
        p Multiply-With-Carry 알고리즘 의사 난수 생성기
        li class Well1024a extends AbstractWell Since 1.0
        p WELL1024a 알고리즘 의사 난수 생성기 + Well19937a, Well19937c, Well44497a, Well44497b, Well512a

    h1 org.apache.commons.rng.core.util
    div
        h2 final class NumberFactory
        +asCode('java').
            static double makeDouble(int v, int w)    // + 오버로딩
            static float makeFloat(int v)
            static long makeLong(int v, int w)        // + 오버로딩
            static int extractHi(long v) + extractLo
            static byte[] makeByteArray(long[] input) // + 오버로딩
            static int makeInt(byte[] input)
            static int[] makeIntArray(byte[] input)   // + makeLongArray
